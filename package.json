    "odin",
    "odin.js"
  ],
  "scripts": {
    "bones": "comn -f ./examples/bones/src/index.js -o ./examples/bones/index.js",
    "particles": "comn -f ./examples/particles/src/index.js -o ./examples/particles/index.js",
    "sound": "comn -f ./examples/sound/src/index.js -o ./examples/sound",
